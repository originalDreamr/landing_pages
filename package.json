{
  "name": "efortles-ssr",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run efortles-ssr:serve-ssr --port 4201",
    "serve:ssr": "node dist/efortles-ssr/server/main.js",
    "build:ssr": "ng build --prod && ng run efortles-ssr:server:production",
    "build:ssr[Optimized]": "ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer && ng run efortles-ssr:server:production",
    "build:ssr[Optimized-W/Hashing]": "ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer --output-hashing none && ng run efortles-ssr:server:production",
    "prerender": "ng run efortles-ssr:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.1.2",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/localize": "^9.0.2",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/platform-server": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@ng-bootstrap/ng-bootstrap": "^6.0.0",
    "@nguniversal/express-engine": "^9.0.1",
    "@ngx-lite/json-ld": "^0.6.2",
    "@ngx-share/core": "^7.1.4",
    "angular": "^1.8.2",
    "bootstrap": "^4.4.1",
    "express": "^4.15.2",
    "jquery": "^3.4.1",
    "localstorage-polyfill": "^1.0.1",
    "ng2-pdf-viewer": "^6.3.0",
    "ngx-captcha": "^8.0.1",
    "ngx-cookie-service": "^2.4.0",
    "ngx-spinner": "^9.0.2",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@nguniversal/builders": "^9.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.30",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
